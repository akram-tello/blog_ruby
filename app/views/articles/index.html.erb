<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= article.body %>
    </li>
<%= link_to "destroy", article_path(article),  method: :delete, data: { confirm: "Are you sure?" } %>

  <% end %>
</ul>
<%= link_to "Create a New Article", new_article_path, class: "new_article" %>